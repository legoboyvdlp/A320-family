<PropertyList>

	<path>res/a320.nlg.ac</path>
	
	<model>
		<name>Nose Tires</name>
		<path>Aircraft/A320-family/Models/LandingGears/a320.nlg.tires.xml</path>
		<offsets>
			<x-m>-0.3061469</x-m>
			<z-m>-1.9329357</z-m>
			<pitch-deg>9</pitch-deg>
		</offsets>
	</model>


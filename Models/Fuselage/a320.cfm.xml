			<and>
				<greater-than>
					<property>environment/relative-humidity</property>
					<value>45</value>
				</greater-than>
				<less-than-equals>
					<property>position/altitude-agl-ft</property>
					<value>75</value>
				</less-than-equals>
				<less-than-equals>
					<property>sim/time/sun-angle-rad</property>
					<value>1.63</value>
				</less-than-equals>
				<greater-than>
					<property>engines/engine[3]/n1</property>
					<value>65</value>
				</greater-than>
			</and>
		</condition>
		<offsets>
			<x-m>-7.432</x-m>
			<y-m>-5.63029</y-m>
			<z-m>-2.46943</z-m>
			<pitch-deg>0</pitch-deg>

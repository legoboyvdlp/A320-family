 			<red>1</red>
 			<green>1</green>
 			<blue>1</blue>
 			<factor-prop>controls/lighting/DU/mcdu2</factor-prop>
 		</emission>
 	</animation>

	<effect>
		<inherits-from>Aircraft/A320-family/Models/Effects/display</inherits-from>
		<object-name>mcdu2.screen</object-name>
	</effect>

	<animation>

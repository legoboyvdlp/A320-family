					<property>/FMGC/internal/flex</property>
					<entry><ind> 70</ind><dep>0.797</dep></entry>
					<entry><ind> 40</ind><dep>0.822</dep></entry>
					<entry><ind>-60</ind><dep>1.000</dep></entry>
				</table>

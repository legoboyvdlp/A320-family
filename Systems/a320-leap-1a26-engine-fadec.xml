			<test value="0">
				/systems/fadec/alpha-floor-switch gt 0
			</test>
			<test logic="AND" value="1">
				/it-autoflight/output/athr eq 1
				/systems/fadec/eng-out eq 1
				/systems/fadec/max-detent le 4
				/systems/fadec/max-detent ge 1
			</test>
			<test logic="AND" value="1">
				/it-autoflight/output/athr eq 1
				/systems/fadec/max-detent eq 2
			</test>
			<test logic="AND" value="1">
				/it-autoflight/output/athr eq 1
				/systems/fadec/max-detent eq 1
			</test>
		</switch>
		
		<pure_gain name="/systems/fadec/athr/output">
			<input>/systems/fadec/athr/cmd</input>
			<gain>1.0</gain>
			<clipto>
				<min>/systems/fadec/limit/idle-norm</min>
				<max>/systems/fadec/athr/max</max>
			</clipto>
		</pure_gain>
		
		<!-- Throttle Finals -->
		<fcs_function name="/systems/fadec/thr-locked-n1">
			<function>
				<ifthen>
					<eq>
						<property>/systems/fadec/thr-locked</property>
						<value>1</value>
					</eq>
					<property>/systems/fadec/thr-locked-n1</property>
					<max>
						<property>/engines/engine[0]/n1-actual</property>
						<property>/engines/engine[1]/n1-actual</property>
					</max>
				</ifthen>
			</function>
		</fcs_function>
		
		<fcs_function name="/systems/fadec/thr-locked-norm">
			<function>
				<table>
					<independentVar lookup="row">/systems/fadec/thr-locked-n1</independentVar>
					<tableData>
						 20.1  0
						101.0  1
					</tableData>
				</table>
			</function>
		</fcs_function>
		
		<pure_gain name="/systems/fadec/control-1/throttle-clamped">
			<input>/systems/fadec/control-1/lever-norm</input>
			<gain>1.0</gain>
			<clipto>
				<min>/systems/fadec/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</pure_gain>
		
		<switch name="/systems/fadec/control-1/throttle-output">
			<default value="/systems/fadec/control-1/lever-norm"/>
			<test value="/systems/fadec/reverse-1/throttle-rev">
				/systems/fadec/reverse-1/active eq 1
			</test>
			<test value="/systems/fadec/limit/toga-norm">
				/systems/fadec/alpha-floor-switch gt 0
			</test>
			<test value="/systems/fadec/thr-locked-norm">
				/systems/fadec/thr-locked eq 1
			</test>
			<test value="/systems/fadec/athr/output">
				/systems/fadec/athr/active eq 1
			</test>
			<test value="/systems/fadec/control-1/throttle-clamped"> <!-- Last -->
				/gear/gear[0]/wow eq 0
			</test>
		</switch>
		
		<lag_filter name="/systems/fadec/control-1/throttle-fdm">
			<input>/systems/fadec/control-1/throttle-output</input>
			<c1>0.75</c1>
			<output>fcs/throttle-pos-norm[0]</output>
		</lag_filter>
		
		<pure_gain name="/systems/fadec/control-2/throttle-clamped">
			<input>/systems/fadec/control-2/lever-norm</input>
			<gain>1.0</gain>
			<clipto>
				<min>/systems/fadec/limit/idle-norm</min>
				<max>1.0</max>
			</clipto>
		</pure_gain>
		
		<switch name="/systems/fadec/control-2/throttle-output">
			<default value="/systems/fadec/control-2/lever-norm"/>
			<test value="/systems/fadec/reverse-2/throttle-rev">
				/systems/fadec/reverse-2/active eq 1
			</test>
			<test value="/systems/fadec/limit/toga-norm">
				/systems/fadec/alpha-floor-switch gt 0
			</test>
			<test value="/systems/fadec/thr-locked-norm">
				/systems/fadec/thr-locked eq 1
			</test>
			<test value="/systems/fadec/athr/output">
				/systems/fadec/athr/active eq 1
			</test>
			<test value="/systems/fadec/control-2/throttle-clamped"> <!-- Last -->
				/gear/gear[0]/wow eq 0
			</test>
		</switch>
		
		<lag_filter name="/systems/fadec/control-2/throttle-fdm">
			<input>/systems/fadec/control-2/throttle-output</input>
			<c1>0.75</c1>
			<output>fcs/throttle-pos-norm[1]</output>
		</lag_filter>
	
	</channel>
	

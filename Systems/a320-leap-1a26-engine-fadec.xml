	<channel name="Engine Control"> <!-- Very complicated, please don't touch if you don't understand it -->
		
		<!-- Engine 1 -->
		<switch name="/systems/fadec/control-1/throttle-pos">
			<default value="/controls/engines/engine[0]/throttle"/>
			<test value="0">
				/systems/fadec/reverse-1/active eq 1
			</test>
		</switch>
		
		<lag_filter name="/systems/fadec/control-1/throttle-lag">
			<input>/systems/fadec/control-1/throttle-pos</input>
			<c1>20</c1>
		</lag_filter>
		
		<fcs_function name="/systems/fadec/control-1/idle-climb-n1">
			<function>
				<sum>
					<property>/systems/fadec/limit/min-n1</property>
					<product>
						<difference>
							<property>/systems/fadec/control-1/throttle-lag</property>
							<property>/systems/acconfig/options/fadec/idle-t</property>
						</difference>
						<quotient>
							<difference>
								<property>/systems/fadec/limit/climb</property>
								<property>/systems/fadec/limit/min-n1</property>
							</difference>
							<difference>
								<property>/systems/acconfig/options/fadec/climb-b</property>
								<property>/systems/acconfig/options/fadec/idle-t</property>
							</difference>
						</quotient>
					</product>
				</sum>
			</function>
			<clipto>
				<min>/systems/fadec/limit/min-n1</min>
				<max>/systems/fadec/limit/climb</max>
			</clipto>
		</fcs_function>
		
		<fcs_function name="/systems/fadec/control-1/climb-mct-n1">
			<function>
				<sum>
					<property>/systems/fadec/limit/climb</property>
					<product>
						<difference>
							<property>/systems/fadec/control-1/throttle-lag</property>
							<property>/systems/acconfig/options/fadec/climb-t</property>
						</difference>
						<quotient>
							<difference>
								<property>/systems/fadec/limit/mct</property>
								<property>/systems/fadec/limit/climb</property>
							</difference>
							<difference>
								<property>/systems/acconfig/options/fadec/mct-b</property>
								<property>/systems/acconfig/options/fadec/climb-t</property>
							</difference>
						</quotient>
					</product>

		<announcement>
			<name>EmerCall</name>
			<mode>once</mode>
			<path>Aircraft/A320-family/Sounds/tripleBelt.wav</path>
			<condition>
				<equals>
					<property>/controls/switches/emerCall</property>
					<value>1</value>
				</equals>
			</condition>
			<volume>
				<factor>8</factor>
			</volume>
			<reference-dist>20.0</reference-dist>
			<max-dist>100.0</max-dist>
